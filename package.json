{
  "name": "myledguitarapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "build:ios": "expo run:ios",
    "build:android": "expo run:android",
    "clean": "expo prebuild --clean",
    "build:testflight": "./scripts/build-testflight.sh",
    "build:testflight:no-submit": "./scripts/build-testflight.sh --no-submit",
    "build:internal": "./scripts/build-testflight.sh --profile preview --no-submit",
    "eas:build:ios": "npx eas-cli build --platform ios --profile production",
    "eas:build:ios:preview": "npx eas-cli build --platform ios --profile preview",
    "eas:build:ios:internal": "npx eas-cli build --platform ios --profile preview",
    "eas:submit": "npx eas-cli submit --platform ios --profile production",
    "eas:whoami": "npx eas-cli whoami",
    "eas:login": "npx eas-cli login",
    "eas:install": "npm install -g eas-cli",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='.*\\.test\\.ts$'",
    "test:integration": "jest --testPathPattern='(configDomainController|ownership|analytics|errorHandling)\\.test\\.ts$'",
    "test:components": "jest --testPathPattern='screens/.*\\.test\\.tsx$'"
  },
  "dependencies": {
    "@expo/cli": "^0.24.20",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~53.0.20",
    "expo-apple-authentication": "^7.2.4",
    "expo-blur": "^13.0.3",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-haptics": "^13.0.1",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-ble-plx": "^3.5.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.2",
    "@babel/preset-typescript": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
